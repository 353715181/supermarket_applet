.detail {
  margin-bottom: 30rpx;
  padding: 0 20rpx;
  background-color: #ffffff;
}
.detail .title .van-cell .van-cell__title {
  color: #7a7a7a;
}
.detail .title .van-cell .van-cell__value {
  color: #4b4b4b;
}
.detail .content {
  background-color: #ffffff;
}
.detail .content .scroll-view_H {
  border-radius: 20rpx;
  white-space: nowrap;
  padding: 20rpx;
  width: 670rpx;
  height: 160rpx;
  background-color: #f5f5f5;
}
.detail .content .scroll-view_H .scroll-view-item_H {
  display: inline-block;
  margin-right: 20rpx;
}
.detail .content .van-image {
  margin-right: 20rpx;
}
.detail .van-image {
  margin-right: 20rpx;
}
.detail .data {
  height: 110rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.detail .data .num {
  font-size: small;
  color: #8f8f8f;
}
.detail .line {
  margin: 0 auto;
  width: 100%;
  height: 2rpx;
  background-color: #f5f5f5;
}
.detail .button_list {
  height: 100rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.detail .button_list .button {
  margin: 20rpx 0;
}
.detail .confirm_button {
  margin: 20rpx 0;
  height: 40px;
  border-radius: 999px;
}
